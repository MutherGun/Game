<GameScreen>:
    BoxLayout:
        orientation: 'vertical'

        FloatLayout:
            id: container
            size_hint_y: 0.8

        Label:
            id: output_label
            text:
            size_hint_y: 0.2
            text_size: self.width, None
            halign: "left"
            valign: "top"
            padding: 10, 10
